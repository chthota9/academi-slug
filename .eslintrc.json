{
    "extends": "eslint:recommended",
    "rules": {
        "no-label-var": "error",
        "no-undef-init": "error",
        

        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "array-bracket-newline": ["error", "never"],
        "array-bracket-spacing": ["error", "never"],
        "no-trailing-spaces": "error",
        "padded-blocks": ["error", "never"],
        "no-console": ["error", { "allow": ["log"] }]
    }, 
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "browser": true,
        "node": true
    }
}